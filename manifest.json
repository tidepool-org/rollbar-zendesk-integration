{
  "name": "Rollbar",
  "author": {
    "name": "Gerrit Niezen",
    "email": "gerrit@tidepool.org",
    "url": "https://github.com/tidepool-org/rollbar-zendesk-integration"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html"
    }
  },
  "domainWhitelist": ["api.rollbar.com"],
  "parameters": [
    {
      "name": "token",
      "type": "text",
      "secure": true,
      "required": true
    },
    {
      "name": "account_name",
      "type": "text",
      "required": true
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
